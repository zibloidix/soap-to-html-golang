											</xs:annotation>
											<xs:complexType>
												<xs:sequence>
													<xs:element name="AnswererGUID" type="base:GUIDType">
														<xs:annotation>
															<xs:documentation>Идентификатор исполнителя по обращению</xs:documentation>
														</xs:annotation>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
