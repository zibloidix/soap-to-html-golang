				</xs:element>
				<xs:element name="ApartmentNumber" type="tns:PremisesNumType" minOccurs="0">
					<xs:annotation>
						<xs:documentation>Номер помещения</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Email" type="tns:String100Type" minOccurs="0">
