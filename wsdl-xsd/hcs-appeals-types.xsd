	<xs:simpleType name="String10000Type">
		<xs:annotation>
			<xs:documentation> Скалярный тип. Строка не более 10 000 символов.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="10000"/>
		</xs:restriction>
