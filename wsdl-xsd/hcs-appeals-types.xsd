	<!--Простые типы-->
	<xs:simpleType name="String100Type">
		<xs:annotation>
			<xs:documentation>Скалярный тип. Строка не более 100 символов.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="100"/>
