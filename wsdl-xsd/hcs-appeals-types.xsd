					<xs:documentation>Лицо - исполнитель обращения</xs:documentation>
				</xs:annotation>
				<xs:complexType>
					<xs:sequence>
						<xs:element name="AnswererGUID" type="base:GUIDType">
							<xs:annotation>
								<xs:documentation>Идентификатор исполнителя</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
			<xs:element name="AnswerText" type="tns:String10000Type">
				<xs:annotation>
					<xs:documentation>Текст ответа на обращение</xs:documentation>
