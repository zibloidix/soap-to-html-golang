														</xs:annotation>
													</xs:element>
													<xs:element name="AnswererGUID" type="base:GUIDType">
														<xs:annotation>
															<xs:documentation>Идентификатор исполнителя по обращению</xs:documentation>
														</xs:annotation>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
									</xs:sequence>
								</xs:sequence>
							</xs:complexType>
						</xs:element>
					</xs:sequence>
					<xs:attribute ref="base:version" use="required" fixed="12.2.0.13"/>
				</xs:extension>
			</xs:complexContent>
		</xs:complexType>
		<xs:key name="importAppealAnswer_TransportGUIDKey">
			<xs:annotation>
				<xs:documentation>Проверка уникальности транспортного идентификатора в рамках запроса</xs:documentation>
			</xs:annotation>
