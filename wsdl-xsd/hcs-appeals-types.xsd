		</xs:key>
		<xs:key name="importAppealAction_AppealNumberKey">
			<xs:annotation>
				<xs:documentation>Проверка уникальности номера обращения в рамках AppealAction</xs:documentation>
			</xs:annotation>
			<xs:selector xpath="tns:AppealAction/tns:AppealNumber"/>
			<xs:field xpath="."/>
		</xs:key>
	</xs:element>
	<!--Типы асинхронного обмена-->
	<xs:element name="getStateResult">
