		<xs:key name="importAppealAnswer_AnswerGUID">
			<xs:annotation>
				<xs:documentation>Проверка уникальности номера обращения в рамках AppealAnswer</xs:documentation>
			</xs:annotation>
			<xs:selector xpath="tns:AnswerAction/tns:Answer/tns:AnswerGUID"/>
